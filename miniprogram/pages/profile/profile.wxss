/* pages/profile/profile.wxss */
/* //父元素 */
.profile{
  background:#e54847;
  height:400rpx;
  width:100%;
  text-align: center;
  position:relative;
}
.profile-img{
  position: absolute;
  width:200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
  left:50%;
  transform:translate(-50%,60rpx);
}